<h1>Editing todo_item</h1>

<% form_for(@todo_item) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :todo_list_id %><br />
    <%= f.text_field :todo_list_id %>
  </p>
  <p>
    <%= f.label :person_id %><br />
    <%= f.text_field :person_id %>
  </p>
  <p>
    <%= f.label :text %><br />
    <%= f.text_field :text %>
  </p>
  <p>
    <%= f.label :position %><br />
    <%= f.text_field :position %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @todo_item %> |
<%= link_to 'Back', todo_items_path %>
